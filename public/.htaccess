# Habilitar el motor de reescritura
RewriteEngine On

# Definir la base de la reescritura. Asegúrate de que esto coincida con la ruta de tu aplicación.
RewriteBase /

# Regla para manejar el enrutamiento de la SPA
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.html [L]

# php -- BEGIN cPanel-generated handler, do not edit
